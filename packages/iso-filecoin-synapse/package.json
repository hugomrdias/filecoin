{
  "name": "iso-filecoin-synapse",
  "type": "module",
  "version": "0.0.5",
  "description": "Actions and hooks for Filecoin Synapse.",
  "author": "Hugo Dias <hugomrdias@gmail.com> (hugodias.me)",
  "funding": "https://github.com/sponsors/hugomrdias",
  "license": "MIT",
  "homepage": "https://github.com/hugomrdias/filecoin/tree/main/packages/iso-filecoin-synapse",
  "repository": {
    "url": "hugomrdias/filecoin",
    "directory": "packages/iso-filecoin-synapse"
  },
  "bugs": {
    "url": "https://github.com/hugomrdias/filecoin/issues"
  },
  "keywords": [
    "filecoin",
    "synapse",
    "dapp",
    "react",
    "hooks",
    "context"
  ],
  "sideEffects": false,
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist/src/**/*.js",
    "dist/src/**/*.d.ts",
    "dist/src/**/*.d.ts.map"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./actions": {
      "types": "./dist/src/actions/index.d.ts",
      "default": "./dist/src/actions/index.js"
    },
    "./gen": {
      "types": "./dist/src/gen.d.ts",
      "default": "./dist/src/gen.js"
    },
    "./chains": {
      "types": "./dist/src/chains.d.ts",
      "default": "./dist/src/chains.js"
    },
    "./types": {
      "types": "./dist/src/types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "chains": [
        "dist/src/chains"
      ],
      "actions": [
        "dist/src/actions/index"
      ],
      "gen": [
        "dist/src/gen"
      ],
      "types": [
        "dist/src/types"
      ]
    }
  },
  "scripts": {
    "lint": "pnpm run build && biome check --no-errors-on-unmatched --files-ignore-unknown=true",
    "test": "playwright-test --runner mocha",
    "build": "tsc --build",
    "generate": "wagmi generate",
    "watch": "tsc --watch"
  },
  "peerDependencies": {
    "viem": "2.x",
    "wagmi": "2.x"
  },
  "dependencies": {
    "@filoz/synapse-sdk": "^0.33.0",
    "@tanstack/react-query": "^5.90.2",
    "@wagmi/core": "2.22.1",
    "dnum": "^2.15.0",
    "ethers": "^6.15.0",
    "filsnap-adapter": "^3.3.8",
    "iso-filecoin": "workspace:*",
    "iso-web": "^1.4.0",
    "multiformats": "^13.4.1",
    "ox": "^0.9.12",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.2.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.7.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@wagmi/cli": "^2.7.0",
    "abitype": "^1.1.1",
    "assert": "^2.1.0",
    "mocha": "^11.7.4",
    "playwright-test": "^14.1.12",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "type-fest": "^5.0.1",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "provenance": false
  },
  "depcheck": {
    "specials": [
      "bin"
    ],
    "ignorePatterns": [
      "dist",
      ".wireit"
    ],
    "ignores": [
      "@types/*",
      "assert",
      "mocha",
      "playwright-test",
      "type-fest"
    ]
  }
}
